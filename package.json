{
  "name": "node_tes_client",
  "version": "1.0.0",
  "description": "NodeJS library to be used with Fund3 Omega",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \\\"Error: no test specified\\\" && exit 1",
    "build": "npm run remove_old_lib && npm run build_client && npm run bundle_protocol",
    "remove_old_lib": "rm -r lib",
    "build_client": "./node_modules/.bin/babel src -d lib/",
    "bundle_protocol": "cp -R src/CommunicationProtocol lib/CommunicationProtocol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fund3/node_tes_client.git"
  },
  "author": "Anthony Wittemann, Calvin Leung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fund3/node_tes_client/issues"
  },
  "homepage": "https://github.com/fund3/node_tes_client#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "capnp": "^0.3.1",
    "capnp-js": "^0.2.11",
    "uuid": "^3.3.2",
    "zeromq": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-plugin-root-import": "^6.1.0"
  }
}
